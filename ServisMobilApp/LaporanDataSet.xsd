                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT p.Nama AS NamaPelanggan, p.Telepon AS TeleponPelanggan, k.NomorPlat, k.Merek, k.Model, lsrv.NamaLayanan, m.Nama AS NamaMekanik, lsrv.Harga AS HargaLayanan, ps.TanggalPesan, ls.TanggalSelesai, ls.BiayaTambahan, ps.Status, lsrv.Harga + ls.BiayaTambahan AS TotalHarga
FROM  LaporanServis AS ls INNER JOIN
         PemesananServis AS ps ON ls.ID_Pemesanan = ps.ID_Pemesanan INNER JOIN
         Pelanggan AS p ON ps.ID_Pelanggan = p.ID_Pelanggan INNER JOIN
         Kendaraan AS k ON ps.ID_Kendaraan = k.ID_Kendaraan INNER JOIN
         LayananServis AS lsrv ON ps.ID_Layanan = lsrv.ID_Layanan LEFT OUTER JOIN
         Mekanik AS m ON ps.ID_Mekanik = m.ID_Mekanik
ORDER BY ls.TanggalSelesai DESC, ls.ID_Laporan</CommandText>
